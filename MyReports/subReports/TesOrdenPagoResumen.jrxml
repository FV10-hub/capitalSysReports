	<parameter name="p_empresa_id" class="java.lang.Long"/>
	<parameter name="p_pago_id" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT 
tp.cod_tipo || ' - ' || tp.descripcion AS tipo_valor,
v.nro_valor,
CASE WHEN tp.descripcion ILIKE '%CHEQUE%' THEN per.nombre_completo ELSE 'EFECTIVO' END AS entidad,
ban.cod_banco AS nro_cuenta,
mon.descripcion AS moneda,
TO_CHAR(v.fecha_valor, 'DD/MM/YYYY') AS fec_valor,
c.monto_total_pago
FROM tes_pagos_cabecera c
JOIN tes_pagos_valores v 
ON c.bs_empresa_id = v.bs_empresa_id 
AND c.id = v.tes_pagos_cabecera_id
JOIN bs_tipo_valor tp 
ON v.bs_empresa_id = tp.bs_empresa_id 
AND v.bs_tipo_valor_id = tp.id
LEFT JOIN tes_bancos ban 
ON v.bs_empresa_id = ban.bs_empresa_id 
AND v.tes_banco_id = ban.id
LEFT JOIN bs_persona per 
ON ban.bs_persona_id = per.id
LEFT JOIN bs_moneda mon 
ON ban.bs_moneda_id = mon.id
WHERE c.bs_empresa_id = $P{p_empresa_id}
AND c.id = $P{p_pago_id}
ORDER BY ban.cod_banco ASC;]]>
	</queryString>
