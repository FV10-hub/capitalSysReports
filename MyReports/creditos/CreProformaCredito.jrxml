	<queryString>
		<![CDATA[select bp.documento,
bp.nombre_completo nombre,
to_char(c.fecha_desembolso,'DD/MM/YYYY') fec_comprobante,
c.nro_desembolso,
'( '|| ta.cod_tipo||' ) ' || ta.descripcion tipo_amortizacion,
round(coalesce(c.monto_total_credito ,0),2) tot_comprobante,
d.nro_cuota, 
to_char(d.fecha_vencimiento,'DD/MM/YYYY') fec_vencimiento,
round(coalesce(d.monto_capital, 0), 2) monto_capital,
round(coalesce(d.monto_interes, 0), 2) monto_interes,
round(coalesce(d.monto_iva, 0), 2) monto_iva,
round(coalesce(d.monto_cuota, 0), 2) monto_cuota
from cre_desembolso_cabecera c, 
cre_desembolso_detalle d,
cre_solicitudes_creditos sc,
cre_tipo_amortizaciones ta,
cob_clientes cl,
bs_persona bp
where c.bs_empresa_id = $P{p_empresa_id} 
and c.id = $P{p_desembolso_id}
and c.id = d.cre_desembolso_cabecera_id  
and c.bs_empresa_id = cl.bs_empresa_id
and c.bs_empresa_id = sc.bs_empresa_id 
and c.cre_solicitud_credito_id = sc.id
and c.cre_tipo_amortizacion_id = ta.id 
and sc.cob_cliente_id = cl.id
and cl.id_bs_persona = bp.id


]]>
	</queryString>
