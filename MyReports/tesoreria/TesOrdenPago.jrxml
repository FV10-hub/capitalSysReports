	<queryString>
		<![CDATA[select 
bp.nombre_completo beneficiario,
bp.documento,
c.tipo_operacion,
cre.nro_desembolso::text nro_comprobante,
to_char(c.fecha_pago,'DD/MM/YYYY') fec_comprobante,
1 nro_cuota,
c.monto_total_pago monto_comprobante
from tes_pagos_cabecera c,
tes_pago_comprobante_detalle d,
cre_desembolso_cabecera cre,
cre_solicitudes_creditos sol,
cob_clientes cc,
bs_persona bp
where c.bs_empresa_id = $P{p_empresa_id}
and c.id =  $P{p_pago_id}
and c.id = d.tes_pagos_cabecera_id 
and c.bs_empresa_id = cc.bs_empresa_id
and c.id_beneficiario = cc.id
and cc.id_bs_persona = bp.id
and cc.bs_empresa_id = cre.bs_empresa_id 
and cre.cre_solicitud_credito_id = sol.id 
and sol.cob_cliente_id = cc.id
and d.id_cuota_saldo = cre.id
and c.tipo_operacion = 'DESEMBOLSO'
union all
select 
bp.nombre_completo nombre,
bp.documento,
c.tipo_operacion,
cs.nro_comprobante_completo,
to_char(c.fecha_pago,'DD/MM/YYYY') fec_comprobante,
cs.nro_cuota,
c.monto_total_pago
from tes_pagos_cabecera c,
tes_pago_comprobante_detalle d,
com_proveedores cc,
bs_persona bp,
com_saldos cs 
where c.bs_empresa_id = $P{p_empresa_id} 
and c.id =  $P{p_pago_id}
and c.id = d.tes_pagos_cabecera_id 
and c.bs_empresa_id = cc.bs_empresa_id 
and c.id_beneficiario = cc.id
and cc.bs_persona_id = bp.id 
and c.tipo_operacion = 'FACTURA'
and c.bs_empresa_id = cs.bs_empresa_id
and cc.id = cs.com_proveedor_id
and d.id_cuota_saldo = cs.id ]]>
	</queryString>
