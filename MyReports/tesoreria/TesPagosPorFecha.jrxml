	<queryString>
		<![CDATA[select 
bp.nombre_completo nombre,
bp.documento,
c.tipo_operacion,
c.nro_pago_completo,
to_char(c.fecha_pago,'DD/MM/YYYY') fec_comprobante,
c.monto_total_pago,
tp.cod_tipo || ' - ' || tp.descripcion tipo_valor,
per.nombre_completo as banco,
v.nro_valor
from tes_pagos_cabecera c,
tes_pago_comprobante_detalle d,
tes_pagos_valores v,
cob_clientes cc,
bs_persona bp,
bs_tipo_valor tp,
tes_bancos ban,
bs_persona per
where c.bs_empresa_id = $P{p_empresa_id} 
and ((c.fecha_pago between to_Date($P{p_fecha_inicio},'dd/MM/yyyy') and  to_date($P{p_fecha_fin},'dd/MM/yyyy') ) 
or ($P{p_fecha_inicio} is null and  $P{p_fecha_fin} is null))
and c.id = d.tes_pagos_cabecera_id 
and c.bs_empresa_id = v.bs_empresa_id 
and c.id = v.tes_pagos_cabecera_id 
and c.bs_empresa_id = cc.bs_empresa_id
and c.id_beneficiario = cc.id
and cc.id_bs_persona = bp.id 
and v.bs_empresa_id = tp.bs_empresa_id 
and v.bs_tipo_valor_id = tp.id
and v.bs_empresa_id = ban.bs_empresa_id 
and v.tes_banco_id = ban.id 
and ban.bs_persona_id = per.id 
and c.tipo_operacion = 'DESEMBOLSO'
union all
select 
bp.nombre_completo nombre,
bp.documento,
c.tipo_operacion,
c.nro_pago_completo,
to_char(c.fecha_pago,'DD/MM/YYYY') fec_comprobante,
c.monto_total_pago,
tp.cod_tipo || ' - ' || tp.descripcion tipo_valor,
per.nombre_completo as banco,
v.nro_valor
from tes_pagos_cabecera c,
tes_pago_comprobante_detalle d,
tes_pagos_valores v,
cob_clientes cc,
bs_persona bp,
bs_tipo_valor tp,
tes_bancos ban,
bs_persona per
where c.bs_empresa_id = $P{p_empresa_id} 
and ((c.fecha_pago between to_Date($P{p_fecha_inicio},'dd/MM/yyyy') and  to_date($P{p_fecha_fin},'dd/MM/yyyy') ) 
or ($P{p_fecha_inicio} is null and  $P{p_fecha_fin} is null)) 
and c.id = d.tes_pagos_cabecera_id 
and c.bs_empresa_id = v.bs_empresa_id 
and c.id = v.tes_pagos_cabecera_id 
and c.bs_empresa_id = cc.bs_empresa_id
and c.id_beneficiario = cc.id
and cc.id_bs_persona = bp.id 
and v.bs_empresa_id = tp.bs_empresa_id 
and v.bs_tipo_valor_id = tp.id
and v.bs_empresa_id = ban.bs_empresa_id 
and v.tes_banco_id = ban.id 
and ban.bs_persona_id = per.id 
and c.tipo_operacion = 'FACTURA'
order by 4 asc]]>
