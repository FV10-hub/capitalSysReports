	<queryString>
		<![CDATA[SELECT 
    m.id                  AS id,
    coalesce(bp.nombre_completo,'EFECTIVO') 	  as banco,
    'INGRESOS'            AS tipo_registro,
    tv.cod_tipo           AS tipo_valor,
    m.nro_comprobante_completo AS nro_comprobante,
    m.tipo_comprobante    AS tipo_comprobante,
    m.nro_valor           AS nro_valor,
    to_char(m.fecha_valor,'DD/MM/YYYY')         AS fecha_valor,
    m.fecha_vencimiento   AS fecha_vencimiento,
    m.monto_cuota         AS monto_valor
FROM cob_cobros_valores m
JOIN bs_tipo_valor tv ON tv.id = m.bs_tipo_valor_id
left join bs_persona bp on m.bs_persona_juridica = bp.id 
WHERE m.estado = 'ACTIVO'
  AND m.ind_depositado = 'S'
  AND m.bs_empresa_id = $P{p_empresa_id}
  and ((m.fecha_valor between to_Date($P{p_fecha_inicio},'dd/MM/yyyy') and  to_date($P{p_fecha_fin},'dd/MM/yyyy') ) 
  or ($P{p_fecha_inicio} is null and  $P{p_fecha_fin} is null))
  AND m.ind_conciliado = $P{p_ind_conciliado}
  AND ( (m.bs_persona_juridica = $P{p_persona_id}) OR (tv.cod_tipo = 'EFE') )
UNION ALL
SELECT 
    p.id                  AS id,
    per.nombre_completo   as banco,
    'EGRESOS'             AS tipo_registro,
    tv.cod_tipo           AS tipo_valor,
    ''                    AS nro_comprobante,
    p.tipo_operacion      AS tipo_comprobante,
    p.nro_valor           AS nro_valor,
    to_char(p.fecha_valor,'DD/MM/YYYY')         AS fecha_valor,
    p.fecha_vencimiento   AS fecha_vencimiento,
    p.monto_cuota         AS monto_valor
FROM tes_pagos_valores p
JOIN bs_tipo_valor tv ON tv.id = p.bs_tipo_valor_id
JOIN tes_bancos b      ON b.id = p.tes_banco_id
JOIN bs_persona per   ON per.id = b.bs_persona_id
WHERE p.bs_empresa_id = $P{p_empresa_id}
  and ((p.fecha_valor between to_Date($P{p_fecha_inicio},'dd/MM/yyyy') and  to_date($P{p_fecha_fin},'dd/MM/yyyy') ) 
  or ($P{p_fecha_inicio} is null and  $P{p_fecha_fin} is null))
  AND p.ind_conciliado = $P{p_ind_conciliado}
  AND b.bs_moneda_id = (SELECT id FROM bs_moneda WHERE cod_moneda = 'GS')
  AND ( (per.id = $P{p_persona_id}) OR (tv.cod_tipo = 'EFE') )
order by 7 asc 
]]>
